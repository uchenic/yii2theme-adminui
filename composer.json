{
  "name": "uchenic/yii2theme-adminui",
  "description": "Yii2 admin ui based on AdminLTE free template",
  "keywords": ["yii2","theme","adminUi","layout"],
  "type": "yii2-extension",
  "license": "GPLv2",
  "support": {
    "issues": "https://github.com/Mihai-p/yii2theme-adminUI/issues?page=1&state=open",
    "source": "https://github.com/uchenic/yii2theme-adminui"
  },
  "authors": [
    {
      "name": "Mithun Mandal",
	  "email":"mihai.petrescu@gmail.com",
      "homepage": "http://2ezweb.com.au"
    }
  ],
  "minimum-stability": "stable",
  
  "require": {
    "yiisoft/yii2":">= 2.0.0",
  	"yiisoft/yii2-bootstrap": ">= 2.0.0",
  	"bower-asset/font-awesome": "4.2.0",
    "bower-asset/Ionicons":"1.5.2",
    "bower-asset/ckeditor":"4.*",
    "bower-asset/bootstrap-daterangepicker":"1.3.13",
    "bower-asset/tinymce":"4.*",
    "bower-asset/dropzone":"3.*",
    "bower-asset/jquery-ui":"*"
  },
  "autoload": {
    "psr-4": {
      "yii\\adminUi\\": "",
  	  "vendor\\adminUi\\assets\\":"assets",
  	  "backend\\themes\\adminui\\":"themes"
    }
  },
  "extra":{
	 "bootstrap":"yii\\adminUi\\AdminUiBootstrap"
  }
}
